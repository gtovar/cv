<h1>Listing proyects</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Start time</th>
      <th>End time</th>
      <th>Position</th>
      <th>Activities</th>
      <th>Outstaning</th>
      <th>Tools</th>
      <th>Description</th>
      <th>Cover</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @proyects.each do |proyect| %>
      <tr>
        <td><%= proyect.name %></td>
        <td><%= proyect.start_time %></td>
        <td><%= proyect.end_time %></td>
        <td><%= proyect.position %></td>
        <td><%= proyect.activities %></td>
        <td><%= proyect.outstaning %></td>
        <td><%= proyect.tools %></td>
        <td><%= proyect.description %></td>
        <td><%= proyect.cover %></td>
        <td><%= link_to 'Show', proyect %></td>
        <td><%= link_to 'Edit', edit_proyect_path(proyect) %></td>
        <td><%= link_to 'Destroy', proyect, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Proyect', new_proyect_path %>
